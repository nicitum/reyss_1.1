{
  "expo": {
    "name": "Reyss",
    "slug": "reyss",
    "scheme": "reyss-app",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/SL_1.jpg",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/SL_1.jpg",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SL_1.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.reyss.reyssapp",
      "permissions": ["android.permission.INTERNET"],
      "config": {
        "usesPermissions": false
      }
    },
    "web": {
      "favicon": "./assets/SL_1.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "8dff7e57-31f9-4b60-9668-d16e77f803c5"
      }
    },
    "owner": "ak_013",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ]
    ]
  }
}