{
  "expo": {
    "name": "Reyss",
    "slug": "reyss",
    "scheme": "reyss-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/SL.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/SL.png",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/SL.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.smoke221.reyssApp",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/SL.png"
    },
    "extra": {
      "eas": {
        "projectId": "026bd023-4864-4eae-bfa1-1e50fd5b6b6f"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs access to your photos to let you report defective products.",
          "cameraPermission": "The app needs access to your camera to let you report defective products."
        }
      ]
    ],
    "owner": "zic_013"
  }
}
